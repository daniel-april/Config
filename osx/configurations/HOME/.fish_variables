set --export LC_ALL en_US.UTF-8
set --export LANG en_US.UTF-8
set --export GIT_WORKSPACE ~/git

set --export EDITOR "subl -w"

set --export AIRFLOW_HOME ~/airflow

set --export PS1 "[\[\033[0;31m\]\t>\[\033[0;34m\]\h\[\033[0m\]:\[\033[0;37m\]\[\033[0;32m\]\u\[\033[0m\]]\w: "
set --export CLICOLOR 1
set --export LSCOLORS Exfxcxdxbxegedabagacad

set --export JAVA_HOME (/usr/libexec/java_home)
set --export LD_LIBRARY_PATH $JAVA_HOME/jre/lib/server
set --export PATH $PATH:$JAVA_HOME/bin

set --export PATH /usr/local/sbin $PATH
set --export PATH ~/.local/bin $PATH

set --export MY_NAME (whoami)

